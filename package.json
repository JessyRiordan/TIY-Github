  "main": "index.js",
  "scripts": {
    "postinstall": "npm run bower:install",
    "bower:install": "bower install --save",
    "postbower:install": "wiredep --src src/index.html",
    "test": "browser-sync start --server --files='src/js/*.js,src/test/index.html,test/spec/**/*.js' --startPath src/test/index.html",
    "start": "browser-sync start --server --files='src/index.html, src/css/*.css, src/js/*.js' --startPath src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JessyRiordan/TIY-Github.git"
  },
